{
    "name": "gigerit/laravel-mysql-optimizer",
    "type": "library",
    "description": "A laravel package that optimizes mysql/mariadb database tables.",
    "keywords": [
        "database",
        "laravel",
        "optimize",
        "mysql",
        "mariadb",
        "table"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "gigerIT",
            "role": "Author"
        }
    ],
    "require": {
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/bus": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.25",
        "orchestra/testbench": "*",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-laravel": "*",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "MySQLOptimizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MySQLOptimizer\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "vendor/bin/pint",
        "test": "vendor/bin/pest",
        "test:types": "vendor/bin/phpstan analyse src --memory-limit=1G"
    }
}
